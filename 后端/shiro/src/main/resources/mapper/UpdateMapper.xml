<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhiyi.shiro.mapper.UpdateMapper">
    <update id="SaveTokenToUserByUN"
            parameterType="com.zhiyi.shiro.model.Shiro_User">
		UPDATE shiro_user set token=#{token}
		where  userName=#{userName}
	</update>

</mapper>
